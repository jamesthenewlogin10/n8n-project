[{"updatedAt":"2025-11-16T11:40:41.488Z","createdAt":"2025-11-16T11:00:27.050Z","id":"sBX4YdKzH6HoHw2Y","name":"My First workflow","active":false,"isArchived":false,"nodes":[{"parameters":{},"type":"n8n-nodes-base.manualTrigger","typeVersion":1,"position":[-480,-176],"id":"76f9bec1-8a81-4235-85b2-4e14f5ef8985","name":"When clicking ‘Execute workflow’"},{"parameters":{"url":"https://jsonplaceholder.typicode.com/todos/1","options":{}},"type":"n8n-nodes-base.httpRequest","typeVersion":4.3,"position":[-272,-176],"id":"135ba1d1-efe1-422e-9fbf-ddefef9f3044","name":"HTTP Request"},{"parameters":{"operation":"write","fileName":"/data/todo.json","options":{"append":false}},"type":"n8n-nodes-base.readWriteFile","typeVersion":1,"position":[96,-176],"id":"3a3bbfdf-77ac-499c-8044-6523d111ce8b","name":"Read/Write Files from Disk"},{"parameters":{"jsCode":"// Function node: create binary property named \"data\"\nreturn items.map(item => {\n  // Convert JSON (or any text) to a Buffer, then to base64 for n8n binary format\n  const text = JSON.stringify(item.json, null, 2);         // or any string you want\n  const buffer = Buffer.from(text, 'utf8');\n  const b64 = buffer.toString('base64');\n\n  item.binary = {\n    data: {\n      data: b64,                  // base64 string\n      fileName: 'todo.json',      // filename that will be used by Write Binary File\n      mimeType: 'application/json'\n    }\n  };\n\n  return item;\n});\n"},"type":"n8n-nodes-base.code","typeVersion":2,"position":[-96,-176],"id":"602d06da-0e53-49af-8e66-a7bdbbd1359b","name":"Code in JavaScript"}],"connections":{"When clicking ‘Execute workflow’":{"main":[[{"node":"HTTP Request","type":"main","index":0}]]},"HTTP Request":{"main":[[{"node":"Code in JavaScript","type":"main","index":0}]]},"Code in JavaScript":{"main":[[{"node":"Read/Write Files from Disk","type":"main","index":0}]]}},"settings":{"executionOrder":"v1"},"staticData":null,"meta":null,"pinData":{},"versionId":"76826aa1-ba61-43fa-8547-dd7cbaa2c7ce